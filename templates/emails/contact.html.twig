
{% block body %}

    <h2>Objet du Message : {{ contact.objet }} </h2>

    <div class="jumbotron">
        <div class="row">
            <div class="col-md-offset-3 col-md-6">
                <h3>Message : {{ contact.message }}</h3></div>
        </div>
        <div class="row">
            <div class="col-md-offset-3 col-md-6">Nom : {{ contact.firstname }} <br>
                Prenom : {{ contact.lastname }}  <br>
                Téléphone : {{ contact.phone }} <br>
                Rue : {{ contact.numDeRue }} {{ contact.rue }} <br>
                Ville : {{ contact.ville }}, {{ contact.cp }}
        </div>
    </div>



{% endblock %}